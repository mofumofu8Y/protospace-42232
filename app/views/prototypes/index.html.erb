<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
       <p class="greeting__text">
  こんにちは、<%= link_to "#{current_user.name}さん", user_path(current_user), class: :greeting__link %>
</p>
      </div>
    <% end %>

    <div class="card__wrapper">
      <%= render partial: 'prototype', collection: @prototypes %> <!-- ✨ここだけシンプルに！ -->
    </div>
  </div>
</main>
